  "Reverse a matrix": {
    "prefix": "reversematrix",
    "body": [
      "int rev_arr[row][col];",
      "  for (int i = row - 1; i >= 0; i--) {",
      "    for (int j = col - 1; j >= 0; j--) {",
      "      rev_arr[row - i - 1][col - j - 1] = arr[i][j];",
      "    }",
